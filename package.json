{
  "name": "coeus",
  "private": true,
  "description": "GDSP",
  "licensed": "unlicensed",
  "scripts": {
    "start": "yarn run run-unchecked",
    "build": "tsc",
    "run-unchecked": "TS_NODE_CACHE=false ts-node -T src/main.ts",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "test": "mocha -r ts-node/register --file tests/test-init.ts tests/*/**/*.ts",
    "test-watch": "mocha --watch-extensions ts --watch -r ts-node/register --file tests/test-init.ts tests/*/**/*.ts",
    "coverage": "nyc -r lcov -e .ts -x \"*.ts\" npm run test"
  },
  "author": "19h",
  "license": "UNLICENCED",
  "dependencies": {
    "@types/bunyan": "^1.8.6",
    "@types/chai": "^4.2.11",
    "@types/express": "^4.17.3",
    "@types/mocha": "^7.0.2",
    "@types/sinon": "^7.5.2",
    "bunyan": "^1.8.12",
    "chai": "^4.2.0",
    "express": "^4.17.1",
    "mocha": "^7.1.1",
    "nyc": "^15.0.0",
    "sinon": "^9.0.1",
    "ts-node": "^8.8.1",
    "tslint": "^6.1.0",
    "tslint-sonarts": "^1.9.0",
    "typescript": "^3.8.3",
    "uuid-v4-wasm": "^0.5.0"
  }
}
